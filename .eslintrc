{
	"root": true,
	"extends": ["@bark", "plugin:jest/recommended"],
	"parserOptions": {
	  "ecmaVersion": 9
	},
	"env": {
	  "es6": true,
	  "node": true
	},
	"globals": {
	  "Promise": true
	},
	"rules": {
	  "camelcase": 0,
	  "indent": [2, "tab", {
		"SwitchCase": 1
	  }],
	  "no-confusing-arrow": 1
	}
  }